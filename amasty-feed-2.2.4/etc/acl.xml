<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Amasty_Feed::general" title="Amasty Product Feed" sortOrder="161812"/>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Catalog::catalog">
                    <resource id="Amasty_Feed::feed" title="Amasty: Product Feed" sortOrder="30">
                        <resource id="Amasty_Feed::feed_profile" title="Profile" sortOrder="10"/>
                        <resource id="Amasty_Feed::feed_category" title="Categories" sortOrder="20"/>
