<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
<acl>
    <resources>
        <resource id="Magento_Backend::admin">
            <resource id="Magento_Backend::stores">
                <resource id="Magento_Backend::stores_settings">
                    <resource id="Magento_Config::config">
                        <resource id="Magecomp_Emailquotepro::config_emailquote" title="Email Cart"/>
                    </resource>
                </resource>
            </resource>
            <resource id="Magecomp_Emailquotepro::emailproductquote" title="Email Cart Pro" sortOrder="10">
                <resource id="Magecomp_Emailquotepro::emailquotepro_emailproductquote" title="Email Cart statistics"
                          sortOrder="20"/>
                <resource id="Magecomp_Emailquotepro::config_emailquotesettings" title="Configuration"
                          sortOrder="30"/>
            </resource>
        </resource>
    </resources>
</acl>
</config>