<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
        <css src="Webkul_Marketplace::css/wk_block.css"/>
        <css src="Webkul_Marketplace::css/style.css"/>
        <css src="Webkul_Marketplace::css/collection.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Webkul\Marketplace\Block\Profile" name="marketplace_sellerprofile" template="Webkul_Marketplace::profile.phtml" before="-">
                <block class="Webkul\Marketplace\Block\Profile" name="marketplace_seller_top_block" template="Webkul_Marketplace::seller/top-block.phtml"/>
                <block class="Webkul\Marketplace\Block\Profile" name="marketplace_sellerprofile_recently_added_products" template="Webkul_Marketplace::seller/profile/recently_added_products.phtml"/>
                <block class="Webkul\Marketplace\Block\Profile" name="marketplace_seller_description" template="Webkul_Marketplace::seller/profile/description.phtml"/>
                <block class="Webkul\Marketplace\Block\Profile" name="marketplace_sellerprofile_rating" template="Webkul_Marketplace::seller/profile/rating.phtml"/>
            </block>
        </referenceContainer>
        <referenceContainer name="marketplace_seller_top_block">
            <block class="Webkul\Marketplace\Block\Profile" name="marketplace_seller_profile_contact" template="Webkul_Marketplace::seller/contact.phtml"/>
            <block class="Webkul\Marketplace\Block\Profile" name="marketplace_seller_profile_menu" template="Webkul_Marketplace::seller/menu.phtml" before="-"/>
        </referenceContainer>
    </body>
</page>
