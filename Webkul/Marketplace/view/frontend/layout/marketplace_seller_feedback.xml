            </block>
        </referenceContainer>
        <referenceContainer name="marketplace_seller_top_block">
            <block class="Webkul\Marketplace\Block\Feedback" name="marketplace_seller_profile_contact" template="Webkul_Marketplace::seller/contact.phtml" />
            <block class="Webkul\Marketplace\Block\Feedback" name="marketplace_seller_profile_menu" template="Webkul_Marketplace::seller/menu.phtml"  before="-"/>
        </referenceContainer>
    </body>
</page>
