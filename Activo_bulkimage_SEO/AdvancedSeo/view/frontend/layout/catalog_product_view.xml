<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>         
        <referenceBlock name="header.container">
            <block class="Activo\AdvancedSeo\Block\Product\View\ExtraJs" ifconfig="advancedseo/general/enable" name="extra.js" after="-" template="Activo_AdvancedSeo::extra_js.phtml" />
        </referenceBlock>

        <!-- Remove this to enable Defalt Magento 2 Schema data for product -->
        <attribute name="itemtype" remove="true" />
        <attribute name="itemscope" remove="true" />

    </body> 
</page>